# NIBR Biomni Environment Configuration
# Copy this file to .env and add your actual API keys

# Required: At least one LLM API key must be provided

# OpenAI API Key (for GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for Claude models)  
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Additional LLM providers
AZURE_OPENAI_API_KEY=your_azure_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Optional: AWS Bedrock (if using AWS models)
AWS_BEARER_TOKEN_BEDROCK=your_bedrock_token_here
AWS_REGION=us-east-1

# Biomni Configuration
BIOMNI_SKIP_DOWNLOAD=true
BIOMNI_DATA_PATH=/biomni_data
BIOMNI_LOG_LEVEL=INFO

# Optional: Default LLM settings
DEFAULT_LLM_PROVIDER=openai
DEFAULT_LLM_MODEL=gpt-4